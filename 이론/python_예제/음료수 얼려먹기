# n, m을 공백을 기준으로 구분하여 입력 받기
n, m = map(int, input().split))

graph = [] 
for i in range(n):
  graph.appned(list(map(int, input())))
  
result = 0
for i in range(n):
  for j in range(m):
      #현재 위치에서 DFS수행
      if dfs(i,j) == True:
        result += 1
print(result)


dfs(x,y):
  if x <= -1 or x >= n or y <= -1 or y >= m:
    return False
    
  if graph[x][y] == 0:
    graph[x][y] = 1
    
    dfs(x - 1, y)
    dfs(x, y - 1)
    dfs(x + 1, y)
    dfs(x, y + 1)
    return True
  return False
