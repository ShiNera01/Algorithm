#include <sstream>

record = {"Enter uid1234 Muzi", "Enter uid4567 Prodo","Leave uid1234","Enter uid1234 Prodo","Change uid4567 Ryan"}



for(vector <string> :: iterator iter = record.begin(); iter != record.end(); i++){
stringstream ss(*it);
string cmd, id, name;

ss >> cmd;

